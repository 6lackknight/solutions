{
  "name": "bp_uploader",
  "version": "0.1.0",
  "license": "MIT",
  "engines": {
    "node": ">=12.13"
  },
  "scripts": {
    "build": "tsc",
    "dev": "ts-node src/index.ts",
    "start": "node ./out/index.js",
    "package": "yarn build && pkg ."
  },
  "dependencies": {
    "chalk": "^4.1.1",
    "commander": "^8.0.0",
    "fast-glob": "^3.2.7",
    "fs-extra": "^10.0.0",
    "loglevel": "^1.7.1",
    "node-json-db": "^1.3.0",
    "promptly": "^3.2.0",
    "request": "^2.88.2"
  },
  "devDependencies": {
    "@types/fs-extra": "^9.0.12",
    "@types/promptly": "^3.0.2",
    "@types/request": "^2.48.7",
    "@types/tar-stream": "^2.2.1",
    "pkg": "^5.2.1",
    "ts-node": "^10.0.0",
    "typescript": "^4.3.4"
  },
  "pkg": {
    "scripts": "out/**/*.js",
    "targets": [
      "node12-linux-x64",
      "node12-macos-x64"
    ],
    "outputPath": "bin"
  },
  "bin": "./out/index.js"
}
